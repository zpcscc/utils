{
  "name": "@dxsixpc/utils",
  "version": "0.2.1",
  "description": "工具库",
  "homepage": "https://dxsixpc.github.io/utils",
  "repository": "https://github.com/dxsixpc/utils",
  "license": "MIT",
  "author": {
    "name": "dxsix",
    "email": "dxsixpc@gmail.com"
  },
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "father build",
    "build:watch": "father dev",
    "commit": "git add . && git-cz",
    "deploy": "pnpm run docs:build && pnpm run docs:deploy",
    "dev": "dumi dev",
    "docs:build": "dumi build",
    "docs:deploy": "gh-pages -d docs-dist",
    "doctor": "father doctor",
    "lint": "pnpm run lint:es && pnpm run lint:css",
    "lint:css": "stylelint \"{src,test}/**/*.{css,less}\"",
    "lint:es": "eslint \"{src,test}/**/*.{js,jsx,ts,tsx}\"",
    "prepare": "husky install && dumi setup",
    "prepublishOnly": "father doctor && pnpm run test && pnpm run build",
    "start": "pnpm run dev",
    "test": "jest"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --parser=typescript --write"
    ]
  },
  "config": {
    "commitizen": {
      "path": "@commitlint/cz-commitlint"
    }
  },
  "dependencies": {
    "@babel/runtime": "^7.21.0",
    "axios": "^1.3.4",
    "crypto-js": "^4.1.1",
    "lodash": "^4.17.21"
  },
  "devDependencies": {
    "@ant-design/icons": "^5.0.1",
    "@babel/core": "^7.21.0",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-typescript": "^7.21.0",
    "@commitlint/cz-commitlint": "^17.4.4",
    "@dxsixpc/components": "0.2.1",
    "@dxsixpc/configs": "^0.4.18",
    "@emotion/react": "^11.10.6",
    "@emotion/styled": "^11.10.6",
    "@types/jest": "^29.4.0",
    "@types/lodash": "^4.14.191",
    "@types/node": "^18.15.0",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "antd": "^5.3.0",
    "babel-jest": "^29.5.0",
    "clipboard": "^2.0.11",
    "commitizen": "^4.0.3",
    "dumi": "^2.1.15",
    "eslint": "^8.36.0",
    "father": "^4.1.6",
    "gh-pages": "^5.0.0",
    "husky": "^8.0.3",
    "inquirer": "^8.0.0",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "lint-staged": "^13.2.0",
    "path": "^0.12.7",
    "postcss": ">=8.4.6 <9.0.0",
    "prettier": "^2.8.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-is": ">= 16.8.0",
    "styled-components": "^5.3.8",
    "stylelint": "^15.2.0",
    "ts-node": "^10.9.1",
    "typescript": ">=4.7.0 <5.0.0",
    "webpack": "^5.76.1"
  },
  "peerDependencies": {
    "react": ">=16.9.0",
    "react-dom": ">=16.9.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
