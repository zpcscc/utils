    "jest": "29.7.0",
    "jest-environment-jsdom": "29.7.0",
    "jsdoc-babel": "0.5.0",
    "jsdoc-to-markdown": "8.0.3",
    "lint-staged": "15.2.9",
    "ora": "8.0.1",
    "path": "0.12.7",
    "postcss": "8.4.41",
    "prettier": "3.3.3",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-is": "18.3.1",
    "styled-components": "6.1.12",
    "ts-node": "10.9.2",
