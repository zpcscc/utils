{
  "extends": "@dxsixpc/configs/tsconfig/tsconfig.react.json",
  "compilerOptions": {
    "baseUrl": ".",
    "outDir": "dist",
    "paths": {
      "@@/*": [".dumi/tmp/*"],
      "docs/": ["docs"],
